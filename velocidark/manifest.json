{
    "manifest_version": 3,
    "name": "Velocidark",
    "version": "1.0",
    "description": "Dark-themed video speed controller with multipliers up to 8x.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "icons/icon48.png"
    },
    "permissions": ["activeTab", "scripting"]
  }